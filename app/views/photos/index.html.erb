<p style="color: green"><%= notice %></p>

<h1>Photos</h1>

<div id="photos">
  <% @photos.each do |photo| %>
    <%= render photo %>
    <p>
    <%= link_to "Edit this photo", edit_photo_path(photo) %> |
    <%= link_to "Back to photos", photos_path %>
    </p>
  <% end %>
</div>

<%= link_to "New photo", new_photo_path %>
